{
	"name": "scattercz/consistence-doctrine-symfony",
	"description": "Symfony Bundle integrating Consistence library and Doctrine ORM with Symfony 5 support",
	"keywords": ["consistence", "doctrine", "orm", "symfony", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Vašek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		},
		{
			"name": "Zdeněk Novotný",
			"email": "scatter@scatter.cz",
			"homepage": "https://scatter.cz"
		}
	],
	"require": {
		"php": "~7.3",
		"consistence/consistence-doctrine": "~1.2|~2.0",
		"doctrine/doctrine-bundle": "~1.0|~2.0",
		"symfony/config": "~3.0|~4.0|~5.0",
		"symfony/dependency-injection": "~3.0|~4.0|~5.0",
		"symfony/http-kernel": "~3.0|~4.0|~5.0",
		"symfony/yaml": "~3.0|~4.0|~5.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"php-parallel-lint/php-console-highlighter": "0.5",
		"php-parallel-lint/php-parallel-lint": "1.2",
		"phpunit/phpunit": "8.5.5"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
